{
  "printWidth": 120,
  "proseWrap": "preserve",
  "semi": true,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 4,
  "arrowParens": "always",
  "trailingComma": "es5",
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
